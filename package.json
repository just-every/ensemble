{
    "name": "@just-every/ensemble",
    "version": "0.2.50",
    "description": "LLM provider abstraction layer with unified streaming interface",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/just-every/ensemble.git"
    },
    "keywords": [
        "llm",
        "ai",
        "openai",
        "anthropic",
        "gemini",
        "deepseek",
        "grok",
        "streaming",
        "model-providers"
    ],
    "author": "just-every",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/just-every/ensemble/issues"
    },
    "homepage": "https://github.com/just-every/ensemble#readme",
    "scripts": {
        "build": "tsc -p .",
        "clean": "rimraf dist",
        "prepare": "npm run build",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint '**/*.{ts,tsx}'",
        "lint:fix": "eslint '**/*.{ts,tsx}' --fix",
        "docs": "typedoc"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./test": {
            "import": "./dist/test.js",
            "types": "./dist/test.d.ts"
        },
        "./types": {
            "import": "./dist/types.js",
            "types": "./dist/types.d.ts"
        },
        "./cost_tracker": {
            "import": "./dist/cost_tracker.js",
            "types": "./dist/cost_tracker.d.ts"
        },
        "./model_providers/*": {
            "import": "./dist/model_providers/*.js",
            "types": "./dist/model_providers/*.d.ts"
        },
        "./utils/*": {
            "import": "./dist/utils/*.js",
            "types": "./dist/utils/*.d.ts"
        }
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.53.0",
        "@google/genai": "^1.4.0",
        "dotenv": "^16.5.0",
        "openai": "^5.2.0",
        "sharp": "^0.34.2",
        "zod": "^3.25.56"
    },
    "devDependencies": {
        "@eslint/js": "^9.28.0",
        "@types/node": "^22.15.30",
        "@vitest/coverage-v8": "^3.2.3",
        "rimraf": "^6.0.1",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.34.0",
        "vitest": "^3.2.3",
        "eslint": "^9.28.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.1",
        "@types/uuid": "^10.0.0",
        "typedoc": "^0.28.5"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.42.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
