{
    "name": "@just-every/ensemble",
    "version": "1.0.0",
    "description": "LLM provider abstraction layer with unified streaming interface",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": ["dist", "package.json", "README.md"],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/just-every/ensemble.git"
    },
    "keywords": [
        "llm",
        "ai",
        "openai",
        "anthropic",
        "gemini",
        "deepseek",
        "grok",
        "streaming",
        "model-providers"
    ],
    "author": "just-every",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/just-every/ensemble/issues"
    },
    "homepage": "https://github.com/just-every/ensemble#readme",
    "scripts": {
        "build": "tsc -p .",
        "clean": "rimraf dist",
        "prepare": "npm run build",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./types": {
            "import": "./dist/types.js",
            "types": "./dist/types.d.ts"
        },
        "./cost_tracker": {
            "import": "./dist/cost_tracker.js",
            "types": "./dist/cost_tracker.d.ts"
        },
        "./model_providers/*": {
            "import": "./dist/model_providers/*.js",
            "types": "./dist/model_providers/*.d.ts"
        },
        "./utils/*": {
            "import": "./dist/utils/*.js",
            "types": "./dist/utils/*.d.ts"
        }
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.51.0",
        "@google/genai": "^0.12.0",
        "openai": "^4.100.0",
        "dotenv": "^16.5.0",
        "sharp": "^0.32.0"
    },
    "devDependencies": {
        "typescript": "^5.8.2",
        "@types/node": "^22.14.0",
        "vitest": "^1.0.0",
        "@vitest/coverage-v8": "^1.0.0",
        "rimraf": "^5.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}